
@{
    ViewData["Title"] = "DelComplete";
}
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    <style type="text/css">
        #para{
            display:inline-block;
        }
        #gya {
            display:inline;
        }
    </style>
</head>
<body>
    <h2>修改成功</h2>
    <p id="para"></p>
    <a id="gya" href="https://localhost:5001/Home/ViewNotes">详情页面...</a>
    <script type="text/javascript">
        var num = 5;
        function jump() {
            var obj = document.getElementById("para");
            if (num > 0) {
                obj.innerHTML = num + "秒后跳转到";
                num--;
            }
            if (num == 0) {
                location.assign("https://localhost:5001/Home/ViewNotes");
            }
        }
        setInterval(jump, 1000);
    </script>
</body>
</html>




